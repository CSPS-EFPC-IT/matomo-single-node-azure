{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "applicationCustomFqdn": {
            "value": "myapp.domain.com"
        },
        "armTemplateRepoRawFilesBaseUrl": {
            "value": "https://raw.githubusercontent.com/CSPS-EFPC-IT/SysAdmins-ARMTemplates/master"
        },
        "configurationAuditLogEnabled": {
            "value": "On"
        },
        "configurationAuditLogEvents": {
            "value": "ADMIN,CONNECTION,DCL,DDL,DML,GENERAL,TABLE_ACCESS"
        },
        "configurationCharacterSetServer": {
            "value": "UTF8MB4"
        },
        "configurationCollationServer": {
            "value": "UTF8MB4_0900_AI_CI"
        },
        "databaseAdminPassword": {
            "value": "mySecret"
        },
        "databaseAdminUsername": {
            "value": "myDbAdminUsername"
        },
        "databaseApplicationPassword": {
            "value": "mySecret"
        },
        "databaseApplicationUsername": {
            "value": "matomo"
        },
        "databaseBackupRetentionDays": {
            "value": 7
        },
        "databaseIops": {
            "value": 1000
        },
        "databaseIopsAutoGrow": {
            "value": "Enabled"
        },
        "databaseStorageGb": {
            "value": 256
        },
        "dataDiskSizeGB" : {
            "value": 64
        },
        "deniedIpAddressList" : {
            "value": "10.12.13.14/32,11.12.13.14/32"
        },
        "environmentName": {
            "value": "dev"
        },
        "gatewaySslCertificateUri": {
            "value": "https://mykeyvaultname.vault.azure.net/secrets/my-certificate-name/my-certificate-version"
        },
        "gatewayUserAssignedIdentity": {
            "value": {
                "/subscriptions/my-subscription-id/resourceGroups/my-resource-group-name/providers/Microsoft.ManagedIdentity/userAssignedIdentities/myUAMI": {}
            }
        },
        "genericShellScriptsRepoRawFilesBaseUrl": {
            "value": "https://raw.githubusercontent.com/CSPS-EFPC-IT/generic-shell-scripts/main"
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "highAvailabilityMode": {
            "value": "Disabled"
