            "value": "mySmtpServerPassword"
        },
        "smtpServerPort": {
            "value": 25
        },
        "smtpServerUsername": {
            "value": "mySmtpServerUsername"
        },
        "smtpServerPrivateIp": {
            "value": "255.255.255.255"
        },
        "standbyAvailabilityZone": {
            "value": "zone"
        },
        "storageAccountAllowedIpAddressList": {
            "value": "255.255.255.255,10.10.10.0/24"
        },
        "virtualMachineTags": {
            "value": {
                "tag1": "value1",
                "tag2": "value2"
            }
        },
        "virtualNetworkAddressBase": {
            "value": "255.255.255"
        },
        "webServerAdminEmail": {
            "value": "admin.email@my.domain.com"
        },
        "webServerAdminPassword": {
            "value": "mySecret"
        },
        "webServerAdminUsername": {
            "value": "myWebServerAdminUsername"
        },
        "webServerBackendMaxExecutionTime": {
            "value": 60
        },
        "webServerRebootUtcTime": {
            "value": "05:00"
        },
        "webServerVirtualMachineSize": {
            "value": "Standard_DS2_v2"
        }
