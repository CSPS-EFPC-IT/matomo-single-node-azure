            "value": "https://raw.githubusercontent.com/CSPS-EFPC-IT/matomo-single-node-azure/main"
        },
        "projectShortName": {
            "value": "myProShoName"
        },
        "resourceLocation": {
            "value": "Canada Central"
        },
        "resourceTags": {
            "value": {
                "tag1": "value1",
                "tag2": "value2"
            }
        },
        "smtpServerFqdn": {
            "value": "my.smtp.domain.com"
        },
        "smtpServerFromAddress": {
            "value": "my-from-addresse@my.domain"
